{
    "compilerOptions": {
      "target": "ESNext",
      "allowJs": true,
      "module": "ESNext",
      "skipLibCheck": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "baseUrl": ".",
      "noEmit": true,
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "strict": true,
      "paths": {
        "*": ["./src/@types/*"]
      },
      "jsx": "react-jsx"
    },
  }